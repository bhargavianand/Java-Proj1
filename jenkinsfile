pipeline { 
	agent any
		triggers {
		cron('H/5 * * * *')
		pollSCM 'H H/1 * * *'
	}

parameters {
        choice(choices: ['master', 'B1'], description: 'What branch?', name: 'Select_branch')
    	}
    	stages {  
        	stage ("SCM Chkout-1"){
			 steps {
               			 sh 'ls -l'
           			 }
		}
        }
}
